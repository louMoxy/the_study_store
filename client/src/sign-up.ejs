<form>
    <div class="form-group">
        <label for="user_name">Username</label>
        <input class="form-control" id="user_name" type="text" name="user_name" required>
    </div>
    <div class="form-group">
        <label for="email">Email: </label>
        <input class="form-control" id="email" type="email" name="email" required>
    </div>
    <div class="form-group">
        <label for="password">Password: </label>
        <input class="form-control" id="password" type="password" name="password" required>
    </div>
    <div class="form-group">
        <label for="retype">Retype: </label>
        <input class="form-control" id="retype" type="password" name="retype" required>
    </div>
    <input type="submit" class="btn btn-dark" id="submit">
</form>
<div id='responseText'></div>
